alias c=clear
alias e=vim
alias myip="hostname -I | tr -d ' '"
alias pt="py.test"
alias pyws="sed -i -e 's/^[ \r\t]*$//'"
alias apt-list="grep -h '^deb ' /etc/apt/sources.list /etc/apt/sources.list.d/*.list | sort -u"
alias version="git --version && tmux -V &&  vim --version | head -1"
alias gdmb="git dmb"
alias today="date +%F"
alias now="date +%FT%T"
alias lsvirtualenv="lsvirtualenv -b"
alias wo="workon"
alias gca="echo DON\'T DO THIS; echo $* > /dev/null"
alias vg=vagrant
