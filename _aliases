alias ack="ack-grep --pager=less"
alias c=clear
alias e=vim
alias myip="hostname -I | tr -d ' '"
alias pt="py.test"
alias pyws="sed -i -e 's/^[ \r\t]*$//'"
alias apt-list="grep -h '^deb ' /etc/apt/sources.list /etc/apt/sources.list.d/*.list | sort -u"
alias version="git --version && tmux -V &&  vim --version | head -1"
